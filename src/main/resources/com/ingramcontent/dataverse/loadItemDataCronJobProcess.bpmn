      <bpmn2:script><![CDATA[org.kie.api.runtime.process.WorkflowProcessInstance pi = (org.kie.api.runtime.process.WorkflowProcessInstance)kcontext.getProcessInstance();
com.fasterxml.jackson.databind.ObjectMapper objectMapper = new com.fasterxml.jackson.databind.ObjectMapper();
System.out.println("******** Start Kafka Load Process *********");
String req = "{\"key\":\"t_ingram_dataverse_item_source\",\"message\":\"Run Item Data Load\",\"status\":\"Created\"}";
com.ingramcontent.dataverse.model.Pipeline r = objectMapper.readValue(req, com.ingramcontent.dataverse.model.Pipeline.class);
long processId = kcontext.getProcessInstance().getId();
r.setProcessId(processId); 
r.setCreated_date(java.time.LocalDateTime.now().toString());
System.out.println("Begin: Calling Kafka procedure to load the item data");
String url = "https://de-dataverse-dev.ingramcontent.com/api/pipeline/trigger";
String request = objectMapper.writeValueAsString(r);
kcontext.setVariable("kafkaRequest", request);
String token = "";
Object response = com.ingramcontent.dataverse.consumer.RestWebServiceConsumer.post(url, request, token);
kcontext.setVariable("kafkaResponse", response);
System.out.println("End: Calling Kafka procedure to load the item data");
System.out.println("******** End Kafka Load Process *********");]]></bpmn2:script>
